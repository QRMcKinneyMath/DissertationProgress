@article{
devoto66,
   Author = {Devoto, Ralph Stephen},
   Title = {Transport Properties of Ionized Monatomic Gases},
   Journal = {The Physics of Fluids},
   Volume = {9},
   Number = {6},
   Pages = {1230-1240},
   Abstract = {Expressions are developed with the Chapman-Enskog-Burnett method for the third and fourth approximations to teh thermal conductivity and the diffusion coefficients, and for the second approximation to the visocostiy of multicomponent gas mixtures. Special forms of these expressions are then derived for application to the ionized gas mixtures, in particular to the partially ionized gas.  Convergence of the approximations is checked by computing the properties of fully ionized hydrogen and servral other mixtures where molecules interact inverse-power repulsive potentials.  From the results for hydrogen, it is seen that at least the third approximation should be used for the thermal conductivity and the duffusion conductivity, adn the second approximation for the electrical conductivity and viscosity of ionized gases.  These results contrast with those for un-ionized gases where one lower level of approximation is generally adequate.},
   Keywords = {Cross-Section, Electrical Conductivity, Transport coefficients, monatomic gases},
   Month = {Nov 1},
   Year = {1966} }




@article{
devoto67simplified,
   Author = {Devoto, Ralph Stephen},
   Title = {Simplified Expressions for the Transport Properties of Ionized Monatomic Gases},
   Journal = {The Physics of Fluids},
   Volume = {10},
   Number = {10},
   Pages = {2105-2112},
   Abstract = {Simplified theoretical expressions for the transport properties of ionized gas mixtures are derived within the framework of the Chapman-Enskog-Burnett method.  The properties of equilibrium partially ionized argon are then computed with these expressions and compared with the values obtained with the exact theory.  Agreement is satisfactory.},
   Keywords = {cross-Section, Electrical Conductivity, Transport coefficients, monatomic gases, simplified},
   Month = {Aug 8},
   Year = {1967} }




@article{
devoto67argon,
   Author = {Devoto, Ralph Stephen},
   Title = {Transport Coefficients of Partially Ionized Argon},
   Journal = {The Physics of Fluids},
   Volume = {10},
   Number = {2},
   Pages = {354-364},
   Abstract = {The complete Chapman-Enskog-Burnett expressions for the transport coefficients of multicomponent gas mixtures are applied to the computation of the properties of partially ionized argon.  Studies of the rate of convergence of the approximations to the coefficients show that the third approximation to the thermal conductivity and the second approximation to the viscosity are adequate at all degrees of ionization.  The ordinary ambipolar diffusion coefficient is given to the excellent accuracy by twice the binary ion-atom diffusion coefficient but, at low ionization, apparently not even the fourth approximation to the the electrical conductivity has converged to the true value.  The computed electrical and thermal conductivities are compared with experimental measurements.},
   Keywords = {Cross-Section, Electrical Conductivity, Transport coefficients, argon},
   Month = {Oct 17},
   Year = {1967} }




